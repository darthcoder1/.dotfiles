{
  "remote.SSH.remotePlatform": {
    "dc1-home": "linux",
    "dc1-desktop": "linux"
  },
  "C_Cpp.updateChannel": "Insiders",
  "debug.onTaskErrors": "debugAnyway",
  "http.proxySupport": "off",
  // formatting
  "gitlens.blame.highlight.enabled": false,
  "editor.codeLens": false,
  "gitlens.views.search.enabled": false,
  "gitlens.hovers.enabled": false,
  "gitlens.hovers.currentLine.enabled": false,
  "gitlens.hovers.currentLine.details": false,
  "gitlens.hovers.currentLine.changes": false,
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.annotations.changes": false,
  "gitlens.hovers.annotations.details": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "sync.gist": "be1010d50b90d189e19da1b8a2e7a8b5",
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[rust]": {
    "editor.defaultFormatter": "matklad.rust-analyzer"
  },
  "go.formatTool": "goformat",
  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": false,
  },
  "[typescript]": {
    "editor.formatOnSave": false,
  },
  "debug.allowBreakpointsEverywhere": true,
  "rust-analyzer.completion.addCallArgumentSnippets": false,
  "rust-analyzer.inlayHints.typeHints": false,
  "rust-analyzer.inlayHints.parameterHints": false,
  "rust-analyzer.inlayHints.chainingHints": false,
  "rust-analyzer.inlayHints.enable": false,
  "editor.formatOnSave": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "terminal.integrated.fontFamily": "Menlo for Powerline",
  "eslint.validate": [
    "typescript",
    "typescriptreact"
  ],
  "eslint.codeActionsOnSave.mode": "all",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "window.zoomLevel": 1,
  "security.workspace.trust.untrustedFiles": "open",
}